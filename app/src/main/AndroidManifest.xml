<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_MEDIA_IMAGES" />
    <uses-permission
        android:name="android.permission.READ_EXTERNAL_STORAGE"
        android:maxSdkVersion="32" />
    <uses-permission
        android:name="android.permission.WRITE_EXTERNAL_STORAGE"
        android:maxSdkVersion="32" />

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@drawable/food_image"
        android:label="@string/app_name"
        android:requestLegacyExternalStorage="true"
        android:roundIcon="@drawable/food_image"
        android:supportsRtl="true"
        android:theme="@style/Theme.QuanLyQuanAn"
        android:usesCleartextTraffic="true"
        tools:targetApi="34">
        <activity
            android:name=".activity.MainActivity"
            android:exported="false" />
        <activity
            android:name=".activity.ActivitySignIn"
            android:exported="false" />
        <activity
            android:name=".activity.ActivityChart"
            android:exported="false" />
        <activity
            android:name=".activity.ActivityListTable"
            android:exported="false" />
        <activity
            android:name=".activity.ActivityListCategory"
            android:exported="false" />
        <activity
            android:name=".activity.ActivityListFood"
            android:exported="true" />
        <activity
            android:name=".hao.activity_table_menu"
            android:exported="true" />
        <activity
            android:name=".hao.activity_food_menu"
            android:exported="true" />
        <activity
            android:name=".hao.activity_loadingApp"
            android:exported="true" >
        <intent-filter>
            <action android:name="android.intent.action.MAIN" />

            <category android:name="android.intent.category.LAUNCHER" />
        </intent-filter>
        </activity>
        <activity
            android:name=".hao.activity_show_bill_table"
            android:exported="true" />


        <activity

            android:name=".activity.StartActivity"
            android:exported="true">
        </activity>
    </application>

</manifest>